{
  "name": "iods/php-bones",
  "description": "An environment for PHP projects.",
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=7.4",
    "iods/php-sdk": "dev-develop"
  },
  "config": {
    "vendor-dir": "src/vendor"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "src/app/code",
      "Application\\Library\\": "src/app/lib"
    },
    "files": [
      "src/etc/application.php",
      "src/etc/configs.php"
    ]
  }
}
